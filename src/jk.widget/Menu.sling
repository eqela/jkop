class:

class Entry public
{
	var title public as string
	var handler public as function
}

pvar entries as vector<Entry>

func addEntry(entry as Entry)
{
	assert entry
	if not entries:
		entries = new vector<Entry>
	entries += entry
}

func addEntry(title as string, handler as function)
{
	var e = new Entry()
	e.title = title
	e.handler = handler
	addEntry(e)
}
