import jk.log
import jk.test

class is UnitTest:

func testDoubleMethods
{
	var db1 = Double.forValue(1.1)
	ASSERT(db1.toDouble() == 1.1)
	var db2 = Double.asValue(2, 0.0)
	ASSERT(db2 == 2.0)
	ASSERT(Double.longBitsToDouble(0 as long) == 0.0)
	ASSERT(Double.longBitsToDouble(4620693217682128896) == 8.0)
	ASSERT(Double.doubleToLongBits(8.0) == 4620693217682128896)
}

func execute override as bool
{
	START_TESTS()
	TEST("Double Methods", testDoubleMethods())
	END_TESTS()
}
