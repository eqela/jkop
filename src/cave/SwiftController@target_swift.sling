class is !"UIViewController" abstract:

depend "swift-import:UIKit"

ctor
{
	lang "swift" {{{
		super.init(nibName: nil, bundle: nil)
	}}}
}

lang "swift" {{{
	convenience required init?(coder decoder: Foundation.NSCoder) {
		self.init(coder: decoder)
	}
}}}

func applicationWillTerminate(application as !"UIApplication") abstract
func applicationDidBecomeActive(application as !"UIApplication") abstract
func applicationWillResignActive(application as !"UIApplication") abstract
func application(application as !"UIApplication", userInfo pname didReceiveRemoteNotification as !"[AnyHashable : Any]") abstract
func applicationWithCompletionHandler(application as !"UIApplication", userInfo pname didReceiveRemoteNotification as !"[AnyHashable : Any]", completionHandler pname fetchCompletionHandler as !"@escaping (UIBackgroundFetchResult) -> Void") abstract
func applicationWithLaunchOptions(application as !"UIApplication", launchOptions pname willFinishLaunchingWithOptions as !"[UIApplication.LaunchOptionsKey : Any]" = null) abstract as bool
func applicationDidEnterBackground(application as !"UIApplication") abstract
func applicationWillEnterForeground(application as !"UIApplication") abstract
