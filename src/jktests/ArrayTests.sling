import jk.log
import jk.test

class is UnitTest:

func testArrayMethods
{
	var arr = new string[]["a", "b"]
	ASSERT(Array.asObject(arr))
	ASSERT(Array.isEmpty(arr) == false)
	ASSERT(Array.isNotEmpty(arr))
	ASSERT(Array.contains(arr, "a"))
	ASSERT(Array.toVector(arr))
	var narr = new string[2]
	var cop = Array.copyFrom(narr, arr, 0, 0, 2)
	ASSERT(sizeof arr == sizeof narr)
	var index = 0
	foreach aa in Array.toVector(narr) {
		ASSERT(aa == arr[index])
		index++
	}
}

func execute override as bool
{
	START_TESTS()
	TEST("Array Methods", testArrayMethods())
	END_TESTS()
}
